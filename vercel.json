{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "public/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.ico",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.png",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.svg",
      "use": "@vercel/static"
    },
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ]
}